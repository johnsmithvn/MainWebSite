{
  "name": "MainWebSite",
  "version": "5.0.5",
  "private": true,
  "workspaces": [
    "backend",
    "react-app"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:backend": "npm --workspace backend run dev",
    "dev:react": "npm --workspace react-app run dev",
    "build": "npm run build:backend && npm run build:react",
    "build:backend": "npm --workspace backend run build",
    "build:react": "npm --workspace react-app run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
