# Local environment overrides
# Copy this to .env.local and modify as needed

# Set to true to completely disable HMR and stop ping requests
VITE_DISABLE_HMR=false

# Uncomment to use different HMR settings
# VITE_HMR_HOST=localhost
# VITE_HMR_PORT=3001

# For local development without Tailscale
# VITE_ALLOWED_HOSTS=localhost,127.0.0.1
